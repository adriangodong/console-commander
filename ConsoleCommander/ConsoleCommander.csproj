<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>ConsoleCommander</Title>
    <Description>Command parser framework for console applications</Description>
    <Authors>adrian</Authors>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyName>ConsoleCommander</AssemblyName>
    <PackageId>ConsoleCommander</PackageId>
    <IncludeSymbols>True</IncludeSymbols>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>0.0.0</Version>
    <PackageProjectUrl>https://github.com/adriangodong/console-commander</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/adriangodong/console-commander/blob/master/LICENSE</PackageLicenseUrl>
  </PropertyGroup>

</Project>
